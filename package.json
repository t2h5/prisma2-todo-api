{
  "name": "prisma2-todo-api",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "lint": "tslint src/*.ts",
    "generate": "prisma2 generate",
    "postinstall": "npm -s run generate",
    "introspect": "prisma2 introspect",
    "studio": "prisma2 studio --experimental",
    "dev": "ts-node-dev src/index.ts",
